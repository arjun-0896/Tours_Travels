<p id="notice"><%= notice %></p>

<h1>Itinenaries</h1>

<table>
  <thead>
    <tr>
      <th>Tourname</th>
      <th>State</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @itinenaries.each do |itinenary| %>
      <tr>

        <td><%= itinenary.tourname %></td>
        <td><%= itinenary.state %></td>
        <td><%= itinenary.country %></td>
        <td><%= button_to 'Show', itinenary, method: :get %></td>
        <td><%= button_to 'Edit', edit_itinenary_path(itinenary), method: :get %></td>
        <td><%= button_to 'Destroy', itinenary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Itinenary', new_itinenary_path %>
