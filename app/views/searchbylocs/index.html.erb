<p id="notice"><%= notice %></p>

<h1>Searchbylocs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @searchbylocs.each do |searchbyloc| %>
      <tr>
        <td><%= link_to 'Show', searchbyloc %></td>
        <td><%= link_to 'Edit', edit_searchbyloc_path(searchbyloc) %></td>
        <td><%= link_to 'Destroy', searchbyloc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Searchbyloc', new_searchbyloc_path %>
